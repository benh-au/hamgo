{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016"
    ]
  },
  "filesGlob": [
    "**/*.ts",
		"!node_modules/**",
		"node_modules/@types/**/*.d.ts",
		"node_modules/@angular/**/*.d.ts"
  ]
}
